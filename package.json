    "npm-check": "npm-check --skip-unused -u",
    "prettier": "prettier src/**/*.{ts,html,scss} -c",
    "start": "yarn clean && webpack-dev-server --env.DEVELOPMENT --open",
    "start:node": "yarn clean && webpack --env.DEVELOPMENT && && node dist/audio-network-lite-v0.1.0-rc15.js run.cliNodeExample",
